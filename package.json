{
  "name": "elib-apis",
  "version": "1.0.0",
  "description": "This is an ebook api project",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "test": "cross-env NODE_ENV=testing jest --watch --runInBand",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "start": "ts-node src/index.ts",
    "migration:generate": "typeorm-ts-node-esm migration:generate",
    "migration:run": "typeorm-ts-node-esm migration:run",
    "migration:create": "typeorm-ts-node-esm migration:create"
  },
  "keywords": [
    "rest",
    "api",
    "express"
  ],
  "author": "Priyansh Singh Rajwar <rajwars.priyansh@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/express-jwt": "^7.4.2",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^16.11.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.1",
    "typescript": "4.5.2",
    "typescript-eslint": "^8.7.0"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "express": "^4.21.0",
    "express-jwt": "^8.4.1",
    "express-validator": "^7.2.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  }
}
